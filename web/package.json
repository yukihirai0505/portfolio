{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://yukihirai0505.github.io/portfolio",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "i18next": "11.3.2",
    "i18next-browser-languagedetector": "2.2.0",
    "i18next-xhr-backend": "1.5.1",
    "react-i18next": "^8.3.5",
    "react-scripts": "2.1.0",
    "react-scroll": "^1.7.10"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.pcss -o src/index.css",
    "watch:css": "postcss src/styles/main.pcss -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && npx gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.3",
    "postcss-at-rules-variables": "^0.1.6",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-properties": "^8.0.8",
    "postcss-for": "^2.1.1",
    "postcss-hexrgba": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.3.0",
    "prettier": "^1.14.3"
  }
}
